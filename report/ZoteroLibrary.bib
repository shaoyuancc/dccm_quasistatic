@article{gillChaosSafeTransformation2015,
  title = {From {{Chaos}} to {{Safe Transformation}}: {{The Emotional Intelligence Learning Environment Model}}},
  author = {Gill, Lesley J and Ramsey, Philip L and Leberman, Sarah I},
