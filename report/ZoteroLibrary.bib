@online{hanConvexFormulationFrictional2023,
  title = {A {{Convex Formulation}} of {{Frictional Contact}} between {{Rigid}} and {{Deformable Bodies}}},
  author = {Han, Xuchen and Masterjohn, Joseph and Castro, Alejandro},
