  urldate = {2023-03-13},
  abstract = {The manipulation of unknown objects is a problem of special interest in robotics since it is not always possible to have exact models of the objects with which the robot interacts. This paper presents a simple strategy to manipulate unknown objects using a robotic hand equipped with tactile sensors. The hand configurations that allow the rotation of an unknown object are computed using only tactile and kinematic information, obtained during the manipulation process and reasoning about the desired and real positions of the fingertips during the manipulation. This is done taking into account that the desired positions of the fingertips are not physically reachable since they are located in the interior of the manipulated object and therefore they are virtual positions with associated virtual contact points. The proposed approach was satisfactorily validated using three fingers of an anthropomorphic robotic hand (Allegro Hand), with the original fingertips replaced by tactile sensors (WTS-FT). In the experimental validation, several everyday objects with different shapes were successfully manipulated, rotating them without the need of knowing their shape or any other physical property.},
  issue = {4},
  langid = {english},
  keywords = {manipulation,pressure measurements,robotics,sensors},
  file = {/home/shaoyuan/snap/zotero-snap/common/Zotero/storage/3U9YPB6I/Montaño and Suárez - 2019 - Dexterous Manipulation of Unknown Objects Using Vi.pdf}
}

