@article{pulerwitzChangingGenderNorms2015,
  title = {Changing {{Gender Norms}} and {{Reducing Intimate Partner Violence}}: {{Results From}} a {{Quasi-Experimental Intervention Study With Young Men}} in {{Ethiopia}}},
