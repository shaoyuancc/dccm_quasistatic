@article{huberAvoidingDenseDynamic2022,
  title = {Avoiding {{Dense}} and {{Dynamic Obstacles}} in {{Enclosed Spaces}}: {{Application}} to {{Moving}} in {{Crowds}}},
  shorttitle = {Avoiding {{Dense}} and {{Dynamic Obstacles}} in {{Enclosed Spaces}}},
