@online{suhDifferentiableSimulatorsGive2022,
  title = {Do {{Differentiable Simulators Give Better Policy Gradients}}?},
  author = {Suh, H. J. Terry and Simchowitz, Max and Zhang, Kaiqing and Tedrake, Russ},
