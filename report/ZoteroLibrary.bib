@article{naugleConflictingInformationCompliance2022,
  title = {Conflicting {{Information}} and {{Compliance}} with {{COVID-19 Behavioral Recommendations}}},
  author = {Naugle, Asmeret and Rothganger, Fred and Verzi, Stephen and Doyle, Casey},
  date = {2022},
  journaltitle = {Journal of artificial societies and social simulation},
  volume = {25},
  number = {4},
  publisher = {{Department of Sociology, University of Surrey}},
  location = {{Guildford}},
  issn = {1460-7425},
  abstract = {The prevalence of COVID-19 is shaped by behavioral responses to recommendations and warnings. Available information on the disease determines the population's perception of danger and thus its behavior; this information changes dynamically, and different sources may report conflicting information. We study the feedback between disease, information, and stay-at-home behavior using a hybrid agent-based-system dynamics model that incorporates evolving trust in sources of information. We use this model to investigate how divergent reporting and conflicting information can alter the trajectory of a public health crisis. The model shows that divergent reporting not only alters disease prevalence over time, but also increases polarization of the population's behaviors and trust in different sources of information.},
  langid = {english},
  keywords = {Behavior modification,Behavioural changes,Behavioural responses,COVID-19,Disease,Information sources,Morbidity,Polarization,Public health,Warnings}
}

@article{neunertTrajectoryOptimizationContacts2017,
  title = {Trajectory {{Optimization Through Contacts}} and {{Automatic Gait Discovery}} for {{Quadrupeds}}},
  author = {Neunert, Michael and Farshidian, Farbod and Winkler, Alexander W. and Buchli, Jonas},
  date = {2017-07},
  journaltitle = {IEEE Robotics and Automation Letters},
  volume = {2},
  number = {3},
  pages = {1502--1509},
  issn = {2377-3766},
  doi = {10.1109/LRA.2017.2665685},
  abstract = {In this letter, we present a trajectory optimization framework for whole-body motion planning through contacts. We demonstrate how the proposed approach can be applied to automatically discover different gaits and dynamic motions on a quadruped robot. In contrast to most previous methods, we do not prespecify contact-switches, -timings, -points or gait patterns, but they are a direct outcome of the optimization. Furthermore, we optimize over the entire dynamics of the robot, which enables the optimizer to fully leverage the capabilities of the robot. To illustrate the spectrum of achievable motions, we show eight different tasks, which would require very different control structures when solved with state-of-the-art methods. Using our trajectory optimization approach, we are solving each task with a simple, high level cost function and without any changes in the control structure. Furthermore, we fully integrate our approach with the robot's control and estimation framework such that we are able to run the optimization online. Through several hardware experiments, we show that the optimized trajectories and control inputs can be directly applied to physical systems.},
  eventtitle = {{{IEEE Robotics}} and {{Automation Letters}}},
  keywords = {Cost function,Dynamics,Hardware,motion and path planning,Multilegged robots,Optimal control,optimization and optimal control,Robots,Trajectory},
  file = {/home/shaoyuan/snap/zotero-snap/common/Zotero/storage/CDBTUK3L/Neunert et al. - 2017 - Trajectory Optimization Through Contacts and Autom.pdf;/home/shaoyuan/snap/zotero-snap/common/Zotero/storage/T56NKV5K/7845678.html}
}

@article{nguefackTrajectoryModellingTechniques2020,
  title = {Trajectory {{Modelling Techniques Useful}} to {{Epidemiological Research}}: {{A Comparative Narrative Review}} of {{Approaches}}},
  author = {Nguefack, Hermine Lore Nguena and Page, M. Gabrielle and Katz, Joel and Choiniere, Manon and Vanasse, Alain and Dorais, Marc and Samb, Oumar Malle and Lacasse, Anais},
  date = {2020},
  journaltitle = {Clinical epidemiology},
  volume = {12},
  pages = {1205--1222},
  publisher = {{Dove Medical Press Ltd}},
  location = {{ALBANY}},
  issn = {1179-1349},
  abstract = {Trajectory modelling techniques have been developed to determine subgroups within a given population and are increasingly used to better understand intra- and inter-individual variability in health outcome patterns over time. The objectives of this narrative review are to explore various trajectory modelling approaches useful to epidemiological research and give an overview of their applications and differences. Guidance for reporting on the results of trajectory modelling is also covered. Trajectory modelling techniques reviewed include latent class modelling approaches, ie, growth mixture modelling (GMM), group-based trajectory modelling (GBTM), latent class analysis (LCA), and latent transition analysis (LTA). A parallel is drawn to other individual-centered statistical approaches such as cluster analysis (CA) and sequence analysis (SA). Depending on the research question and type of data, a number of approaches can be used for trajectory modelling of health outcomes measured in longitudinal studies. However, the various terms to designate latent class modelling approaches (GMM, GBTM, LTA, LCA) are used inconsistently and often inter-changeably in the available scientific literature. Improved consistency in the terminology and reporting guidelines have the potential to increase researchers' efficiency when it comes to choosing the most appropriate technique that best suits their research questions.},
  langid = {english},
  keywords = {cluster analysis,Epidemiology,Estimates,group-based trajectory modelling,growth mixture modelling,Health aspects,latent class analysis,latent transition analysis,Life Sciences \& Biomedicine,modelling techniques,Pain,Population,Public Environmental \& Occupational Health,Random variables,Review,Science \& Technology,sequence analysis}
}

@article{nianogoAgentBasedModelingNoncommunicable2015,
